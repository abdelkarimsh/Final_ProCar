@model UserViewModel

<h1>عقود الايجار الخاصة بك  </h1>
<br />
<hr />



@if (Model.lease.Any())
{
    <br />
    <table class="table table-hover">
        <tr>
            <th>حالة العقد</th>
            <th>تاريخ بداية العقد</th>
            <th>تاريخ نهاية العقد</th>
            @*<th>نوع السيارة</th>*@
            <th>السعر </th>
            
        </tr>

        @foreach (var lease in Model.lease)
        {
    <tr>
        <th>@lease.leasestatus</th>
        <th>@lease.StartRent</th>
        <th>@lease.EndRent</th>
        @*<th>@lease.Car.MakerName</th>*@
        <th>@lease.TotalPrice $</th>

        <th><a class="btn btn-primary" asp-controller="Lease" asp-action="Index">تعديل </a></th>
    </tr>
        }
    </table>
}
else
{
    <div class="alert alert-warning">
        No Data in our database !
    </div>

}

