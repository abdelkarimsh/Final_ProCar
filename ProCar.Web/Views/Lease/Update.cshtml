@model UpdateLeaseDto

@{
    ViewData["Title"] = "تعديل عقد الايجار";
}
<form asp-action="Update" class="ajaxForm" tname="#kt_datatable" enctype="multipart/form-data">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
   
    <div class="row">
        <input asp-for="Id" type="hidden" />
        <div class="col-md-6">
            <div class="form-group">
                <label asp-for="StartRent" class="control-label"></label>
                <input asp-for="StartRent" class="form-control" />
                <span asp-validation-for="StartRent" class="text-danger"></span>
            </div>
        </div>
        <div class="col-md-6">
            <div class="form-group">
                <label asp-for="EndRent" class="control-label"></label>
                <input asp-for="EndRent" class="form-control" />
                <span asp-validation-for="EndRent" class="text-danger"></span>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-md-6">
            <div class="form-group">
                <label asp-for="CarId" class="control-label"></label>
                <select asp-for="CarId" class="form-control" asp-items="@ViewBag.Cars">
                </select>
                <span asp-validation-for="CarId" class="text-danger"></span>
            </div>
        </div>
        <div class="col-md-4">
            <div class="form-group">
                <label asp-for="UserId" class="control-label"></label>
                <select asp-for="UserId" class="form-control" asp-items="@ViewBag.Users">
                </select>
                <span asp-validation-for="UserId" class="text-danger"></span>
            </div>
        </div>
    </div>


    <div class="form-group">
        <input type="submit" value="حفظ" class="btn btn-primary" />
        <a data-dismiss="modal" aria-label="Close" class="btn btn-danger">الغاء</a>
    </div>

</form>



<script>
    PageLoadActions();
</script>

<script src="~/lib/jquery-validation/dist/jquery.validate.js"></script>
<script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>

